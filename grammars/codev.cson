# NOTE: Caret(^) is included to word border characters in Atom, while recognized as the prefix of user defined variable in CodeV.

'comment': 'CodeV Macro-plus language support in Atom'
'name': 'CodeV'
'scopeName': 'source.codev'
'fileTypes': [
   'seq'
   'SEQ'
]
'patterns': [
   {
      'match': '!.*$'
      'name': 'comment.line.codev'
   }
   {
      'match': '(?<=[\\s\\-\\+\\*\\/\\\\=:\\(,]|^)\\d*\\.?\\d+([eE][+-]?\\d)?([0-9&&[^\\.]])*\\b'
      'name': 'constant.numeric.codev'
   }
   {
      'begin': '\''
      'end': '\''
      'name': 'string.quoted.single.codev'
   }
   {
      'begin': '\"'
      'end': '\"'
      'name': 'string.quoted.double.codev'
   }
   {
      'match': '\\$\\w+\\b'
      'name': 'string.interpolated.codev'
   }
   {
      'comment': 'user defined variable'
      'match': '(\\^)([a-zA-Z][a-zA-Z0-9_]*)\\b'
      'captures':
         '1':
            'name': 'punctuation.definition.variable.other.codev'
         '0':
            'name': 'variable.other.codev'
   }
   {
      'match': '[#]\\d+\\b'
      'name': 'variable.parameter.codev'
   }
   {
      'comment': 'invalid variable'
      'match': '(\\^)([_0-9]\\w*)\\b'
      'name': 'invalid.illegal.variable.other.codev'
   }
   {
      'match': '(^|;|\\(|\\s)((?i)IF|ELSE|ELS|WHI|FOR|UNT|END|GOT|LBL|FCT)\\b'
      'captures':
         '2':
            'name': 'keyword.control.codev'
   }
   {
      'match': '(^|;|\\(|\\s)((?i)true|false|yes|no|or|and)\\b'
      'captures':
         '2':
            'name': 'constant.language.codev'
   }
   {
      'match': '(^|;|\\(|\\s)((?i)LCL|GBL)\\b'
      'captures':
         '2':
            'name': 'storage.modifier.codev'
   }
   {
      'match': '(^|;|\\(|\\s)((?i)NUM|STR)\\b'
      'captures':
         '2':
            'name': 'storage.type.codev'
   }
   {
      'include': '#qualifiers'
   }
   {
      'include': '#commands'
   }
   {
      'include': '#functions'
   }
]
'repository':
   'qualifiers':
      'patterns':[
         {
            'comment': '''qualifiers;
                           a: aberration
                           b: buffer
                           e: entry
                           f: field
                           g: global
                           i: row
                           j: cloumn
                           q: format
                           r: ray
                           s: surface
                           t: tolerance
                           u: file
                           w: wavelength
                           z: zoom
                        special constant characters;
                           a: all
                           c: current
                           i: image
                           l: last
                           o: object
                           s: stop
                        '''
            'match': '\\s((?i)[abefgijqrstuwz](\\d+|[acilos]))\\b'
            'captures':
               '0':
                  'name': 'constant.language.qualifier.codev'
         }
         {
            'comment': 'qualifier with string label'
            'match': '\\s((?i)[abefgijqrstuwz])((\".*\")|(\'.*\'))'
            'captures':
               '1':
                  'name': 'constant.language.qualifier.codev'
               '3':
                  'name': 'string.quoted.double.codev'
               '4':
                  'name': 'string.quoted.single.codev'
         }
         {
            'comment': 'qualifier with user-defined variable'
            'match': '\\s((?i)[abefgijqrstuwz])(\\^)([a-zA-Z][a-zA-Z0-9_]*)\\b'
            'captures':
               '1':
                  'name': 'constant.language.qualifier.codev'
               '2':
                  'name': 'punctuation.definition.variable.other.codev'
               '3':
                  'name': 'variable.other.codev'
         }
         {
            'comment': 'range qualifier'
            'match': '(\\.{2})(\\d+|(?i)[acilos])\\b'
            'captures':
               '1':
                  'name': 'punctuation.definition.range.qualifier.codev'
               '2':
                  'name': 'constant.language.qualifier.codev'
         }
         {
            'comment': '''as database item or constant, e.g.(num s), out t, etc
                              f: field
                              n: no
                              s: surface
                              t: true
                              w: wavelength
                              y: yes
                              z: zoom
                              '''
            'match': '\\s+((?i)[fnstwyz])\\b'
            'name': 'constant.language.qualifier.codev'
         }
      ]
   'commands':
      'patterns':[
